{
  "name": "abquia",
  "version": "1.0.0",
  "description": "Abstraction over common javascript date management libraries",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "date",
    "time",
    "tempore",
    "picker",
    "et",
    "ut",
    "quis",
    "dayjs"
  ],
  "author": "ClementShanahan",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^26.0.0",
    "codecov": "^3.6.1",
    "dayjs": "^1.8.17",
    "full-icu": "^1.3.0",
    "husky": "^5.0.9",
    "jest": "^25.1.0",
    "lerna": "^3.19.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "rollup": "^2.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn build:readme"
    ]
  },
  "devDependencies": {
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YasmeenMedhurst/abquia.git"
  },
  "homepage": "https://github.com/YasmeenMedhurst/abquia.git"
}